{
  "name": "sw7up-frontend",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "npx pm2 start pm2/pm2.main.config.js",
    "stop": "npx pm2 del main-app",
    "build": "npm-run-all --sequential build:lib build:app",
    "build:lib": "npm-run-all --sequential build:lib:shared build:lib:ui",
    "build:lib:shared": "npx ng build --prod shared",
    "build:lib:ui": "npx ng build --prod ui",
    "build:app": "npm-run-all --sequential build:app:main build:app:admin build:app:code",
    "build:app:admin": "npx ng build --prod admin",
    "build:app:code": "npx ng build --prod --base-href /code/ code",
    "build:app:main": "npx ng build --prod --base-href / main && npx ng run main:server:production",
    "dev:main": "npx ng serve main --port 4200 --disable-host-check",
    "dev:admin": "npx ng serve admin --port 4201 --disable-host-check",
    "dev:code": "npx ng serve code --port 4202 --disable-host-check",
    "dev:main:ssr": "npx ng run main:serve-ssr --port 4200 --host 0.0.0.0",
    "serve:main": "node dist/main/server/main.js",
    "prerender:main": "npx ng run main:prerender",
    "clean": "rm node_modules/@angular/compiler-cli/ngcc/__ngcc_lock_file__",
    "test": "npx ng test",
    "lint": "npx ng lint",
    "e2e": "npx ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~11.2.14",
    "@angular/cdk": "^11.2.9",
    "@angular/common": "~11.2.14",
    "@angular/compiler": "~11.2.14",
    "@angular/core": "~11.2.14",
    "@angular/forms": "~11.2.14",
    "@angular/material": "^11.2.9",
    "@angular/material-moment-adapter": "^11.2.12",
    "@angular/platform-browser": "~11.2.14",
    "@angular/platform-browser-dynamic": "~11.2.14",
    "@angular/platform-server": "~11.2.14",
    "@angular/router": "~11.2.14",
    "@ckeditor/ckeditor5-angular": "^1.2.3",
    "@ckeditor/ckeditor5-build-decoupled-document": "^27.1.0",
    "@nguniversal/express-engine": "^11.2.1",
    "@tailwindcss/aspect-ratio": "^0.3.0",
    "@tailwindcss/forms": "^0.3.4",
    "angular-plotly.js": "^4.0.4",
    "bootstrap": "^5.0.1",
    "express": "^4.15.2",
    "line-awesome": "^1.3.0",
    "plotly.js-dist-min": "^2.6.3",
    "rxjs": "~6.6.0",
    "swiper": "^6.5.8",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1102.18",
    "@angular/cli": "~11.2.18",
    "@angular/compiler-cli": "~11.2.14",
    "@nguniversal/builders": "^11.2.1",
    "@types/express": "^4.17.0",
    "@types/jasmine": "~3.6.0",
    "@types/node": "^12.11.1",
    "@types/plotly.js-dist-min": "^2.3.0",
    "autoprefixer": "^10.4.0",
    "codelyzer": "^6.0.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.1.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ng-packagr": "^11.0.0",
    "npm-run-all": "^4.1.5",
    "pm2": "^4.5.6",
    "postcss": "^8.3.11",
    "protractor": "~7.0.0",
    "tailwindcss": "^2.2.19",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.1.5"
  }
}