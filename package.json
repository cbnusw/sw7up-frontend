{
  "name": "sw7up-frontend",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "npx pm2 start pm2/pm2.main.config.js",
    "stop": "npx pm2 del main-app",
    "build": "run-s build:lib build:app",
    "build:lib": "run-s build:lib:shared build:lib:ui",
    "build:lib:shared": "npx ng build --configuration production shared",
    "build:lib:ui": "npx ng build --configuration production ui",
    "build:app": "run-s build:app:*",
    "build:app:admin": "npx ng build --configuration production admin",
    "build:app:project": "npx ng build --configuration production --base-href /project/ project",
    "build:app:main": "npx ng build --configuration production --base-href / main && npx ng run main:server:production",
    "dev:main": "npx ng serve main --port 4200 --host 0.0.0.0 --disable-host-check",
    "dev:admin": "npx ng serve admin --port 4201 --host 0.0.0.0 --disable-host-check",
    "dev:code": "npx ng serve code --port 4202 --disable-host-check --serve-path=/code/",
    "dev:project": "npx ng serve project --port 4202 --host 0.0.0.0 --disable-host-check --serve-path=/project/",
    "dev:main:ssr": "npx ng run main:serve-ssr --port 4200 --host 0.0.0.0",
    "serve:main": "node dist/main/server/main.js",
    "prerender:main": "npx ng run main:prerender",
    "clean": "rm node_modules/@angular/compiler-cli/ngcc/__ngcc_lock_file__",
    "test": "npx ng test",
    "lint": "npx ng lint",
    "e2e": "npx ng e2e",
    "dev:ssr": "ng run main:serve-ssr",
    "serve:ssr": "node dist/main/server/main.js",
    "build:ssr": "ng build && ng run main:server",
    "prerender": "ng run main:prerender"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~13.1.2",
    "@angular/cdk": "^13.1.2",
    "@angular/common": "~13.1.2",
    "@angular/compiler": "~13.1.2",
    "@angular/core": "~13.1.2",
    "@angular/forms": "~13.1.2",
    "@angular/material": "^13.1.2",
    "@angular/material-moment-adapter": "^13.1.3",
    "@angular/platform-browser": "~13.1.2",
    "@angular/platform-browser-dynamic": "~13.1.2",
    "@angular/platform-server": "~13.1.2",
    "@angular/router": "~13.1.2",
    "@ckeditor/ckeditor5-angular": "^1.2.3",
    "@ckeditor/ckeditor5-build-decoupled-document": "^27.1.0",
    "@nguniversal/express-engine": "^13.0.1",
    "@swimlane/ngx-charts": "^20.0.1",
    "@tailwindcss/forms": "^0.3.4",
    "angular-plotly.js": "^4.0.4",
    "bootstrap": "^5.0.1",
    "bson": "^4.7.0",
    "express": "^4.15.2",
    "line-awesome": "^1.3.0",
    "plotly.js-dist-min": "^2.6.3",
    "rxjs": "~6.6.0",
    "swiper": "^8.3.2",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.1.3",
    "@angular-devkit/core": "^13.1.3",
    "@angular/cli": "~13.1.3",
    "@angular/compiler-cli": "~13.1.2",
    "@nguniversal/builders": "^13.0.1",
    "@types/express": "^4.17.0",
    "@types/jasmine": "~3.6.0",
    "@types/node": "^12.11.1",
    "@types/plotly.js-dist-min": "^2.3.0",
    "autoprefixer": "^10.4.0",
    "codelyzer": "^6.0.0",
    "jasmine-core": "^3.99.0",
    "karma": "~6.3.11",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ng-packagr": "^13.1.3",
    "npm-run-all": "^4.1.5",
    "pm2": "^4.5.6",
    "postcss": "^8.3.11",
    "protractor": "~7.0.0",
    "tailwindcss": "^3.0.13",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.5.4"
  }
}
